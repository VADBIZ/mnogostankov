<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><i class="fa fa-bar-tool-o"></i> {{ heading_title }}</h3>
  </div>
  <div class="panel-body">
    <div class="list-group">
        {% for link in links %}
        <a href="{{ link.href }}" class="list-group-item list-group-item-action">{{ link.name }}</a>
        {% endfor %}
    </div>
  </div>
</div>
